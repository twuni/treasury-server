# ----------------------------------------------------------------------------------------------
# Treasury Configuration
# ----------------------------------------------------------------------------------------------

# This is the domain name on which your treasury will be hosted.
treasury.domain=www.example.com

# This is the character length of identifiers and secrets generated by this treasury.
treasury.strength=2048

# ----------------------------------------------------------------------------------------------
# Database Configuration
# ----------------------------------------------------------------------------------------------
# Important! Don't forget to add the library containing the JDBC driver to your classpath.

repository.jdbc.driver=org.hsqldb.jdbc.JDBCDriver
repository.jdbc.url=jdbc:hsqldb:file:data/treasury
repository.jdbc.username=SA
repository.jdbc.password=
